{
   "version": "0.2.0",
   "configurations": [
      {
         "type": "node",
         "request": "launch",
         "name": "Launch Program",
         "skipFiles": [
            "<node_internals>/**"
         ],
         "program": "${file}",
         "outFiles": [
            "${workspaceFolder}/**/*.ts"
         ]
      },
      {
         "type": "node-terminal",
         "name": "Run Script: dev",
         "request": "launch",
         "command": "npm run dev",
         "cwd": "${workspaceFolder}\\Back"
      }
   ]
}